<?xml version="1.0" encoding="UTF-8"?>
<root>
  <swagger>2.0</swagger>
  <info>
    <version>1.0.0</version>
    <title>Swagger Petstore</title>
    <description>A sample API that uses a petstore as an example to demonstrate features in the swagger-2.0 specification</description>
    <termsOfService>http://swagger.io/terms/</termsOfService>
    <contact>
      <name>Swagger API Team</name>
    </contact>
    <license>
      <name>MIT</name>
    </license>
  </info>
  <host>petstore.swagger.io</host>
  <basePath>/api</basePath>
  <schemes>http</schemes>
  <consumes>application/json</consumes>
  <produces>application/json</produces>
  <paths>
    <pets>
      <get>
        <description>Returns all pets from the system that the user has access to</description>
        <produces>application/json</produces>
        <responses>
          <description>A list of pets.</description>
          <schema>
            <type>array</type>
            <items>
              <ref>#/definitions/Pet</ref>
            </items>
          </schema>
        </responses>
      </get>
    </pets>
  </paths>
  <definitions>
    <Pet>
      <type>object</type>
      <required>id</required>
      <required>name</required>
      <properties>
        <id>
          <type>integer</type>
          <format>int64</format>
        </id>
        <name>
          <type>string</type>
        </name>
        <tag>
          <type>string</type>
        </tag>
      </properties>
    </Pet>
  </definitions>
</root>
